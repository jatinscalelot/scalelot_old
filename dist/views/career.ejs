<div id="Applynow2" class="max-w-screen-2xl mx-auto px-5 lg:px-9 py-12 lg:py-16 xl:py-24 2xl:py-28 items-center hidden">
  <div class="text-center pb-16">
    <h2>Apply your next job at Scalelot</h2>
  </div>
  <form action="/api/v1/career" method="POST" enctype="multipart/form-data">
    <div class="pr-0 px-0 xl:px-28 lg:px-14 mx-auto">
      <div class="flex flex-wrap">
        <div class="w-full sm:w-1/2 sm:px-3.5 mb-5">
          <label for="first-name" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">First Name<span class="text-scalelot-secondary">*</span></label>
          <input type="text" id="firstName" name="firstName" placeholder="First Name" class="dInput small rounded-lg form-control" />
        </div>
        <div class="w-full sm:w-1/2 sm:px-3.5 mb-5">
          <label for="last-name" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">Last Name<span class="text-scalelot-secondary">*</span></label>
          <input type="text" id="lastName" name="lastName" placeholder="Last Name" class="dInput small rounded-lg form-control" />
        </div>
        <div class="w-full sm:w-1/2 sm:px-3.5 mb-5">
          <label for="email-add" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">Email Address<span class="text-scalelot-secondary">*</span></label>
          <input type="email" id="email" name="email" placeholder="Email Address" class="dInput small rounded-lg form-control" />
        </div>
        <div class="w-full sm:w-1/2 sm:px-3.5 mb-5">
          <label for="phone-num" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">Phone Number<span class="text-scalelot-secondary">*</span></label>
          <input type="number" id="phoneNumber" name="phoneNumber" placeholder="Phone Number" class="dInput small rounded-lg form-control" />
        </div>
        <div class="w-full sm:px-3.5 mb-5">
          <label for="address" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">Address<span class="text-scalelot-secondary">*</span></label>
          <textarea id="address" name="address" placeholder="Address" rows="4" class="dInput small rounded-lg form-control"></textarea>
        </div>
        <div class="w-full sm:w-1/2 sm:px-3.5 mb-5">
          <label for="apply-for" class="text-sm lg:text-base text-scalelot-primary dark:text-white font-medium mb-2 block">Apply For<span class="text-scalelot-secondary">*</span></label>
          <input type="text" id="applyFor" name="applyFor" placeholder="Apply For" class="dInput small rounded-lg form-control" />
