<section class="py-12 lg:py-16 xl:py-24 2xl:py-28">
  <div class="max-w-screen-4k mx-auto py-12 lg:py-16 items-center relative">
    <span class="bg-scalelot-primary dark:bg-opacity-25 backdrop-blur-md absolute inset-0 w-10/12 max-w-screen-xl mx-auto rounded-3xl"></span>
    <div class="text-center w-full relative space-y-2 pb-5 text-white">
      <h2 class="text-center text-white">Our Recent Work</h2>
      <p class="max-w-6xl mx-auto px-12">Checkout our recent design and development works</p>
      <div class="w-full flex items-center justify-center space-x-7">
        <div class="navArrowPrev cursor-pointer hover:text-scalelot-secondary anim"><span class="block text-3xl md:text-4xl lg:text-5xl text-current">&#8592;</span></div>
        <span class="h-8 mt-0.5 md:mt-2 border-l-2 border-current opacity-30"></span>
        <div class="navArrowNext cursor-pointer hover:text-scalelot-secondary anim"><span class="block text-3xl md:text-4xl lg:text-5xl text-current">&#8594;</span></div>
      </div>
    </div>
    <!-- Swiper -->
    <div class="w-full mx-auto relative mb-5 xl:mb-10">
      <div class="swiper recentWork relative mb-6">
        <div class="swiper-wrapper">
          <!-- ToDo: Make technologies dynamic using map -->
          <!-- ToDo: [IMP] Instead base64 image, get image from /assets/images/dynamicImages-->
          <% portfolios.forEach(function(portfolio){ %>
            <div class="swiper-slide w-11/12 md:w-1/3 lg:w-1/4">
              <a href="javascript:void(0);" 
              onclick='addActiveD("#pop1", "show", "<%= portfolio.title %>", "<%= portfolio.category %>", "<%= portfolio.description %>", "<%= portfolio.files[0] %>", "<%= portfolio.webFramework %>", "<%= portfolio.programmingLanguages %>", "<%= portfolio.miscellaneous %>", "<%= portfolio.libraries %>", "<%= portfolio.uiFrameworks %>", "<%= portfolio.designingLanguage %>", "<%= portfolio.designingTools %>", "<%= portfolio.rating %>", "<%= portfolio.pageLink %>")'
              class="relative group block overflow-hidden text-gray-100 rounded-xl lg:rounded-3xl">
                <span class="bg-gradient-to-t from-scalelot-jpIndigo to-transparent block absolute z-0 inset-0 w-full h-full opacity-100 md:opacity-0 group-hover:opacity-100"></span>
                <span class="absolute anim w-9 h-[calc(100%+300px)] top-1/2 transform -translate-y-1/2 -rotate-45 bg-white opacity-30 md:opacity-0 group-hover:opacity-30 -right-9 md:right-full group-hover:-right-9 block delay-150"></span>
                <!-- <img src="/assets/images/ourworkimg1.png" class="w-full anim" /> -->
                <img src="/assets/images/dynamic-images/<%= portfolio.files[0] %>" alt="Scalelot Portfolio" class="w-full anim" />
                <div class="absolute inset-x-0 bottom-0 w-full py-4 md:py-8 translate-x-5 md:translate-x-20 pr-7 xl:pr-10 group-hover:translate-x-5 opacity-100 md:opacity-0 group-hover:opacity-100 anim delay-300 flex items-end justify-between">
                  <div class="">
                    <p><%= portfolio.category %></p>
                    <h5 class="font-pop text-white"><%= portfolio.title %></h5>
                  </div>
                  <div class="text-xl whitespace-nowrap"><%= portfolio.rating %> <span class="text-2xl text-yellow-400">&#9733;</span></div>
                </div>
              </a>
            </div>
          <% }) %>
        </div>
        <div class="recentWork-pagination -mb-2"></div>
      </div>
    </div>
    <div class="text-center w-full relative dark">
      <button onclick="window.location='/work/portfolio'" type="button" class="btn-border text-current border-current group anim rounded-full border dark:border-white dark:text-white">
        <span class="bleft anim group-hover:left-0"></span>
        Explore More
        <span class="bright anim group-hover:-right-full"></span>
      </button>
    </div>
  </div>
</section>
